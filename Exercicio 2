def questao2():
    try:
        with open("frases.txt", "r", encoding="utf-8") as f:
            linhas = f.readlines()
        nao_vazias = [l for l in linhas if l.strip() != ""]
        print(f"Linhas não vazias: {len(nao_vazias)}")
    except PermissionError:
        print("Permissão negada ao acessar o arquivo frases.txt")